eswc2019:Conference a conference:Conference ;
	rdfs:label "ESWC 2019"@en ;
	conference:description "The ESWC is a major venue for discussing the latest scientific results and technology innovations around semantic technologies. Building on its past success, ESWC is seeking to broaden its focus to span other relevant related research areas in which Web semantics plays an important role."@en ;
	conference:hasSite eswc2019-sites:Hotel_Bernardin ;
	conference:startDate "2019-06-02T09:00:00.000+02:00"^^xsd:dateTime ;
	conference:endDate "2019-06-06:15:00:00.000+02:00"^^xsd:dateTime ;
	.

eswc2019-sites:Hotel_Bernardin a conference:Site ;
	rdfs:label "Hotel Bernardin"@en ;
	.

fr:ReviewVersion rdfs:label "Review Version"@en ;
	.

<https://easychair.org/> rdfs:label "EasyChair"@en ;
	.

<https://2019.eswc-conferences.org/> rdfs:label "ESWC 2019"@en ;
	.

<http://www.lexvo.org/page/iso639-3/eng> rdfs:label "English"@en ;
	.

eswc2019:Paper a owl:Class ;
	rdfs:label "Paper"@en ;
	.

eswc2019:Track a owl:Class ;
	rdfs:label "Track"@en ;
	.

eswc2019:ProceedingsPaper a owl:Class ;
	rdfs:label "Proceedings Paper"@en ;
	rdfs:subClassOf eswc2019:Paper ;
	.

eswc2019:SubmissionPaper a owl:Class ;
	rdfs:label "Submission Paper"@en ;
	rdfs:subClassOf eswc2019:Paper ;
	.


eswc2019:ReviewRatingScale a bido:ReviewRatingScale ;
	rdfs:label "ESWC 2019 Review Rating Scale [-3, 3]"@en ;
	.

eswc2019:ReviewRatingValue a owl:Class ;
	rdfs:subClassOf bido:ReviewRatingValue ;
	rdfs:label "Review Rating"@en ;
	eswc2019:scale eswc2019:ReviewRatingScale;
	owl:equivalentClass [
		a owl:Class ;
		owl:oneOf (
			eswc2019:ReviewRating.3
			eswc2019:ReviewRating.2
			eswc2019:ReviewRating.1
			eswc2019:ReviewRating.0
			eswc2019:ReviewRating.-1
			eswc2019:ReviewRating.-2
			eswc2019:ReviewRating.-3
		) ;
	] ;
	.

eswc2019:ReviewRating.3 a eswc2019:ReviewRatingValue ;
	rdfs:label "3: Strong Accept"@en ;
	.

eswc2019:ReviewRating.2 a eswc2019:ReviewRatingValue ;
	rdfs:label "2: Accept"@en ;
	.

eswc2019:ReviewRating.1 a eswc2019:ReviewRatingValue ;
	rdfs:label "1: Weak Accept"@en ;
	.

eswc2019:ReviewRating.0 a eswc2019:ReviewRatingValue ;
	rdfs:label "0: Borderline Paper"@en ;
	.

eswc2019:ReviewRating.-1 a eswc2019:ReviewRatingValue ;
	rdfs:label "-1: Weak Reject"@en ;
	.

eswc2019:ReviewRating.-2 a eswc2019:ReviewRatingValue ;
	rdfs:label "-2: Reject"@en ;
	.

eswc2019:ReviewRating.-3 a eswc2019:ReviewRatingValue ;
	rdfs:label "-3: Strong Reject"@en ;
	.



eswc2019:ReviewerConfidenceScale a bido:ReviewerConfidenceScale ;
	rdfs:label "ESWC 2019 Reviewer Confidence Scale [-3, 3]"@en ;
	.

eswc2019:ReviewerConfidenceValue a owl:Class ;
	rdfs:subClassOf bido:ReviewerConfidenceValue ;
	rdfs:label "Reviewer Confidence"@en ;
	eswc2019:scale eswc2019:ReviewerConfidenceScale ;
	owl:equivalentClass [
		a owl:Class ;
		owl:oneOf (
			eswc2019:ReviewerConfidence.5
			eswc2019:ReviewerConfidence.4
			eswc2019:ReviewerConfidence.3
			eswc2019:ReviewerConfidence.2
			eswc2019:ReviewerConfidence.1
		) ;
	] ;
	.

eswc2019:ReviewerConfidence.3 a eswc2019:ReviewerConfidenceValue ;
	rdfs:label "5: Expert"@en ;
	.

eswc2019:ReviewerConfidence.4 a eswc2019:ReviewerConfidenceValue ;
	rdfs:label "4: High"@en ;
	.

eswc2019:ReviewerConfidence.3 a eswc2019:ReviewerConfidenceValue ;
	rdfs:label "3: Medium"@en ;
	.

eswc2019:ReviewerConfidence.2 a eswc2019:ReviewerConfidenceValue ;
	rdfs:label "2: Low"@en ;
	.

eswc2019:ReviewerConfidence.1 a eswc2019:ReviewerConfidenceValue ;
	rdfs:label "1: None"@en ;
	.


# -----------------------------------------------------------------------------
# Predicates; Domain & Range Restrictions
# -----------------------------------------------------------------------------

eswc2019:doi a owl:DatatypeProperty ;
	rdfs:label "DOI"@en ;
	rdfs:domain eswc2019:ProceedingsPaper ;
	rdfs:range eswc2019-datatype:doi ;
	.

eswc2019:pdf a owl:DatatypeProperty ;
	rdfs:label "PDF"@en ;
	rdfs:domain eswc2019:ProceedingsPaper ;
	rdfs:range xsd:anyUri ;
	.

eswc2019:submissionTime a owl:DatatypeProperty ;
	rdfs:label "Time of Submission"@en ;
	rdfs:domain eswc2019:SubmissionPaper ;
	rdfs:range xsd:dateTime ;
	.

eswc2019:authorList a owl:ObjectProperty ;
	rdfs:label "Author List"@en ;
	rdfs:domain eswc2019:Paper ;
	rdfs:range rdf:List ;
	.

eswc2019:submission a owl:ObjectProperty ;
	rdfs:label "Submission"@en ;
	rdfs:domain eswc2019:ProceedingsPaper ;
	rdfs:range eswc2019:SubmissionPaper ;
	.

eswc2019:rebuts a owl:ObjectProperty ;
	rdfs:label "Rebuts"@en ;
	rdfs:domain eswc2019:Rebuttal ;
	rdfs:range eswc2019:SubmissionPaper ;
	.

eswc2019:transparentReviewer a owl:DatatypeProperty ;
	rdfs:label "Transparent Reviewer"@en ;
	rdfs:domain fr:ReviewVersion ;
	rdfs:range xsd:boolean ;
	.

eswc2019:track a owl:ObjectProperty ;
	rdfs:label "Track"@en ;
	rdfs:domain eswc2019:Paper ;
	rdfs:range eswc2019:Track ;
	.

eswc2019:correspondingAuthor a owl:DatatypeProperty ;
	rdfs:label "Corresponding Author"@en ;
	rdfs:domain conference:RoleDuringEvent ;
	rdfs:range xsd:boolean ;
	.

eswc2019:submissionsList a owl:ObjectProperty ;
	rdfs:label "List of Submissions"@en ;
	rdfs:domain eswc2019:Conference ;
	rdfs:range rdfs:List ;
	.

eswc2019:proceedingsList a owl:ObjectProperty ;
	rdfs:label "List of Proceedings"@en ;
	rdfs:domain eswc2019:Conference ;
	rdfs:range rdfs:List ;
	.

eswc2019:tracksInclude a owl:ObjectProperty ;
	rdfs:label "Set of Tracks"@en ;
	rdfs:domain eswc2019:Conference ;
	rdfs:range eswc2019:Track ;
	.

