@base <https://comunica.github.io/Tutorial-ESWC2019-Comunica/> .

@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix eswc2019: <https://metadata.2019.eswc-conferences.org/rdf/ontology/> .
@prefix conference: <https://w3id.org/scholarlydata/ontology/conference-ontology.owl#> .
@prefix role: <https://w3id.org/scholarlydata/role/> .
@prefix hoteli-bernardin: <https://www.hoteli-bernardin.si/mice/bernardin/congress-halls/> .


# -------------------------------------
# Location (informative)
# -------------------------------------
<https://www.hoteli-bernardin.si/mice/bernardin/congress-halls/aurora#section-2> a conference:Site ;
	conference:description "Section II of the Aurora Hall conference room" ;
	.

# -------------------------------------
# the Tutorial itself
# -------------------------------------
<#Tutorial> a conference:Tutorial ;
	conference:isSubEventOf eswc2019:Conference ;
	conference:hasLocation <https://www.hoteli-bernardin.si/mice/bernardin/congress-halls/aurora#section-2> ;
	rdfs:label "Comunica"@en ;
	conference:description "Querying Linked Data on the Web is a non-trivial endeavour because..."@en ;
	conference:hasSubEvent <#Part_1>, <#Break>, <#Part_2> ;
	.

# -------------------------------------
# Persons associated with the Tutorial
# -------------------------------------
<https://www.rubensworks.net/#me> a conference:Person ;
	conference:givenName "Ruben" ;
	conference:familyName "Taelman" ;
	conference:holdsRole <#Organizer> ;
	conference:hasAffiliation <#IDLab> ;
	.

# -------------------------------------
# Temporal-bound affiliations of persons associated with the Tutorial
# -------------------------------------
<#IDLab> a conference:AffiliationDuringEvent ;
	conference:during <#Tutorial> ;
	conference:withOrganization <http://www.ugent.be/ea/idlab/en> ;
	.

# -------------------------------------
# Organizations of affiliations
# -------------------------------------
<http://www.ugent.be/ea/idlab/en> a conference:Organisation ;
	conference:name "IDLab - Ghent University"@en ;
	.

# -------------------------------------
# Roles of persons associated with the Tutorial
# -------------------------------------
<#Organizer> a conference:OrganisationalRoleDuringEvent ;
	conference:during <#Tutorial> ;
	conference:withRole role:tutorial-organiser ;
	.

# -------------------------------------
# Tutorial sessions
# -------------------------------------
<#Part_1> a conference:Session ;
	rdfs:label "Part 1: basic"@en ;
	conference:hasSubEvent <#Intro>, <#Browser>, <#Usage> ;
	.

<#Intro> a conference:Session ;
	rdfs:label "Introduction to Comunica"@en ;
	conference:startDate "2019-06-03T09:00:00.000+02:00"^^xsd:dateTime ;
	conference:endDate "2019-06-03T09:30:00.000+02:00"^^xsd:dateTime ;
	.

<#Browser> a conference:Session ;
	rdfs:label "Browser usage examples"@en ;
	conference:startDate "2019-06-03T09:30:00.000+02:00"^^xsd:dateTime ;
	conference:endDate "2019-06-03T09:45:00.000+02:00"^^xsd:dateTime ;
	.

<#Usage> a conference:Session ;
	rdfs:label "Usage inside an application"@en ;
	conference:startDate "2019-06-03T09:45:00.000+02:00"^^xsd:dateTime ;
	conference:endDate "2019-06-03T010:30:00.000+02:00"^^xsd:dateTime ;
	.

<#Break> a conference:Break ;
	rdfs:label "Break"@en ;
	conference:startDate "2019-06-03T10:30:00.000+02:00"^^xsd:dateTime ;
	conference:endDate "2019-06-03T11:00:00.000+02:00"^^xsd:dateTime ;
	.

<#Part_2> a conference:Session ;
	rdfs:label "Part 2: advanced"@en ;
	conference:hasSubEvent <#Config>, <#Algorithm> ;
	.

<#Config> a conference:Session ;
	rdfs:label "Configuration with Components.js"@en ;
	conference:startDate "2019-06-03T11:00:00.000+02:00"^^xsd:dateTime ;
	conference:endDate "2019-06-03T11:30:00.000+02:00"^^xsd:dateTime ;
	.

<#Algorithm> a conference:Session ;
	rdfs:label "Adding a custom algorithm"@en ;
	conference:startDate "2019-06-03T11:30:00.000+02:00"^^xsd:dateTime ;
	conference:endDate "2019-06-03T12:30:00.000+02:00"^^xsd:dateTime ;
	.

